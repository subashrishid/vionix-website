---
layout: base.njk
pageTitle: "End-to-End IT Solutions & Services"
pageDescription: "Vertaxes Technologies provides complete IT solutions including Web & Mobile App Development, Server AMC, CCTV Installation, Networking, Cloud Backup, Cybersecurity, and Hardware Support."
activePage: "home"
permalink: /
---

<!-- ========== 1. HERO SECTION ========== -->
<section id="hero" class="hero-section">
    <div class="hero-bg-particles" id="heroParticles"></div>
    <div class="hero-gradient-overlay"></div>
    <div class="container hero-inner">
        <div class="hero-content" data-animate="fade-up">
            <div class="hero-badge">
                <span class="badge-dot"></span>
                {{ home.badge }}
            </div>
            <h1 class="hero-title">
                {{ home.title_line1 }} <span class="gradient-text">{{ home.title_highlight }}</span> {{ home.title_line2 }}
            </h1>
            <p class="hero-subtitle">{{ home.subtitle }}</p>
            <div class="hero-buttons">
                <a href="/contact/" class="btn btn-primary btn-glow">
                    <i class="fas fa-headset"></i> Free Consultation
                </a>
                <a href="{{ site.whatsappLink }}" class="btn btn-whatsapp btn-glow" target="_blank">
                    <i class="fab fa-whatsapp"></i> WhatsApp Now
                </a>
            </div>
            <div class="hero-trust">
                <div class="hero-trust-item"><i class="fas fa-shield-halved"></i> Secure & Reliable</div>
                <div class="hero-trust-item"><i class="fas fa-globe"></i> Worldwide Support</div>
                <div class="hero-trust-item"><i class="fas fa-bolt"></i> Fast Response</div>
            </div>
        </div>
        <div class="hero-visual" data-animate="fade-left">
            <div class="hero-3d-container">
                <div class="floating-card card-1"><i class="fas fa-cloud"></i><span>Cloud</span></div>
                <div class="floating-card card-2"><i class="fas fa-server"></i><span>Server</span></div>
                <div class="floating-card card-3"><i class="fas fa-shield-halved"></i><span>Security</span></div>
                <div class="floating-card card-4"><i class="fas fa-code"></i><span>Development</span></div>
                <div class="floating-card card-5"><i class="fas fa-network-wired"></i><span>Network</span></div>
                <div class="hero-center-orb">
                    <div class="orb-ring ring-1"></div>
                    <div class="orb-ring ring-2"></div>
                    <div class="orb-ring ring-3"></div>
                    <div class="orb-core"><i class="fas fa-microchip"></i></div>
                </div>
                <svg class="connection-lines" viewBox="0 0 500 500">
                    <line class="conn-line" x1="250" y1="250" x2="100" y2="80" />
                    <line class="conn-line" x1="250" y1="250" x2="400" y2="100" />
                    <line class="conn-line" x1="250" y1="250" x2="420" y2="300" />
                    <line class="conn-line" x1="250" y1="250" x2="100" y2="380" />
                    <line class="conn-line" x1="250" y1="250" x2="300" y2="430" />
                </svg>
            </div>
        </div>
    </div>
    <div class="hero-wave">
        <svg viewBox="0 0 1440 120" preserveAspectRatio="none">
            <path d="M0,60 C360,120 720,0 1080,60 C1260,90 1380,30 1440,60 L1440,120 L0,120 Z" fill="#F8FAFC" />
        </svg>
    </div>
</section>

<!-- ========== 2. TRUST BADGES / STATS ========== -->
<section id="stats" class="stats-section">
    <div class="container">
        <div class="stats-grid">
            {% for stat in home.stats %}
            <div class="stat-card" data-animate="fade-up" data-delay="{{ loop.index0 * 100 }}">
                <div class="stat-icon"><i class="{{ stat.icon }}"></i></div>
                <div class="stat-number" data-count="{{ stat.count }}">0</div>
                {% if stat.suffix %}<div class="stat-suffix">{{ stat.suffix }}</div>{% endif %}
                <div class="stat-label">{{ stat.label }}</div>
            </div>
            {% endfor %}
        </div>
    </div>
</section>

<!-- ========== 3. SERVICES SECTION ========== -->
<section id="services" class="services-section">
    <div class="container">
        <div class="section-header" data-animate="fade-up">
            <span class="section-tag">What We Offer</span>
            <h2 class="section-title">Our <span class="gradient-text">Premium Services</span></h2>
            <p class="section-desc">Comprehensive IT solutions tailored to elevate your business with cutting-edge technology and expert support.</p>
        </div>
        <div class="services-grid">
            {% for svc in services.services %}
            <div class="service-card" data-animate="fade-up" data-delay="{{ loop.index0 * 50 }}">
                <div class="service-card-img">
                    <img src="{{ svc.image }}" alt="{{ svc.name }}" loading="lazy">
                    <div class="service-card-overlay"></div>
                </div>
                <div class="service-card-body">
                    <div class="service-icon"><i class="{{ svc.icon }}"></i></div>
                    <h3>{{ svc.name }}</h3>
                    <p>{{ svc.desc }}</p>
                    <a href="/contact/" class="btn btn-outline-sm">Send Enquiry <i class="fas fa-arrow-right"></i></a>
                </div>
            </div>
            {% endfor %}
        </div>
    </div>
</section>

<!-- ========== 4. WHY CHOOSE US ========== -->
<section id="why-us" class="why-section">
    <div class="container">
        <div class="why-grid">
            <div class="why-image" data-animate="fade-right">
                <img src="https://images.unsplash.com/photo-1573164713988-8665fc963095?w=700&q=80" alt="IT Professional Team" loading="lazy">
                <div class="why-image-badge">
                    <span class="badge-number">10+</span>
                    <span class="badge-text">Years of Experience</span>
                </div>
            </div>
            <div class="why-content" data-animate="fade-left">
                <span class="section-tag">Why {{ site.companyName }}?</span>
                <h2 class="section-title">Why Choose <span class="gradient-text">{{ site.companyFull }}</span></h2>
                <p class="why-desc">We combine technical expertise with strategic thinking to deliver IT solutions that drive real business results.</p>
                <div class="why-points">
                    <div class="why-point"><div class="why-point-icon"><i class="fas fa-users-gear"></i></div><div><h4>Professional IT Support Team</h4><p>Certified engineers with deep expertise across all IT domains</p></div></div>
                    <div class="why-point"><div class="why-point-icon"><i class="fas fa-indian-rupee-sign"></i></div><div><h4>Affordable Pricing</h4><p>Competitive rates without compromising on quality or service</p></div></div>
                    <div class="why-point"><div class="why-point-icon"><i class="fas fa-gauge-high"></i></div><div><h4>Fast Troubleshooting</h4><p>Quick diagnosis and resolution with minimal business disruption</p></div></div>
                    <div class="why-point"><div class="why-point-icon"><i class="fas fa-earth-americas"></i></div><div><h4>Worldwide Remote Support</h4><p>Connect with our experts from anywhere in the world</p></div></div>
                    <div class="why-point"><div class="why-point-icon"><i class="fas fa-lock"></i></div><div><h4>Secure and Scalable Solutions</h4><p>Enterprise-grade security with solutions that grow with you</p></div></div>
                    <div class="why-point"><div class="why-point-icon"><i class="fas fa-file-contract"></i></div><div><h4>Trusted Server AMC Services</h4><p>Proactive maintenance keeping your infrastructure running 24/7</p></div></div>
                </div>
            </div>
        </div>
    </div>
</section>

<!-- ========== 5. SERVER AMC SECTION ========== -->
<section id="server-amc" class="amc-section">
    <div class="container">
        <div class="section-header" data-animate="fade-up">
            <span class="section-tag">Enterprise Solutions</span>
            <h2 class="section-title">Server AMC & <span class="gradient-text">IT Maintenance</span> Services</h2>
            <p class="section-desc">Keep your servers and IT infrastructure running at peak performance with our comprehensive maintenance contracts.</p>
        </div>
        <div class="amc-grid">
            <div class="amc-visual" data-animate="fade-right">
                <div class="amc-3d-server">
                    <div class="server-unit unit-1"><div class="server-leds"><span class="led led-green"></span><span class="led led-green"></span><span class="led led-blue"></span></div><div class="server-slots"><span></span><span></span><span></span><span></span></div></div>
                    <div class="server-unit unit-2"><div class="server-leds"><span class="led led-green"></span><span class="led led-orange"></span><span class="led led-blue"></span></div><div class="server-slots"><span></span><span></span><span></span><span></span></div></div>
                    <div class="server-unit unit-3"><div class="server-leds"><span class="led led-green"></span><span class="led led-green"></span><span class="led led-green"></span></div><div class="server-slots"><span></span><span></span><span></span><span></span></div></div>
                    <div class="server-pulse"></div>
                </div>
            </div>
            <div class="amc-features" data-animate="fade-left">
                <div class="amc-feature"><div class="amc-feature-icon"><i class="fas fa-desktop"></i></div><div><h4>Server Monitoring</h4><p>24/7 real-time health monitoring and performance analytics</p></div></div>
                <div class="amc-feature"><div class="amc-feature-icon"><i class="fas fa-database"></i></div><div><h4>Backup Setup</h4><p>Automated backup schedules with secure offsite storage</p></div></div>
                <div class="amc-feature"><div class="amc-feature-icon"><i class="fas fa-shield-virus"></i></div><div><h4>Security Updates</h4><p>Regular patches and security updates to prevent vulnerabilities</p></div></div>
                <div class="amc-feature"><div class="amc-feature-icon"><i class="fas fa-microchip"></i></div><div><h4>Hardware & Software Support</h4><p>Complete support for all server hardware and software components</p></div></div>
                <div class="amc-feature"><div class="amc-feature-icon"><i class="fas fa-clock-rotate-left"></i></div><div><h4>Downtime Prevention</h4><p>Proactive measures to ensure maximum uptime and reliability</p></div></div>
                <a href="/server-amc/" class="btn btn-primary btn-glow" style="margin-top:1rem;">
                    <i class="fas fa-headset"></i> Request AMC Support
                </a>
            </div>
        </div>
    </div>
</section>

<!-- ========== 6. CCTV SECURITY SECTION ========== -->
<section id="cctv" class="cctv-section">
    <div class="container">
        <div class="cctv-grid">
            <div class="cctv-content" data-animate="fade-right">
                <span class="section-tag">Security Solutions</span>
                <h2 class="section-title">CCTV Installation & <span class="gradient-text">Surveillance</span></h2>
                <p>Protect your home and business with professional-grade CCTV surveillance systems installed by experts.</p>
                <div class="cctv-points">
                    <div class="cctv-point"><i class="fas fa-check-circle"></i> Home & Office CCTV Setup</div>
                    <div class="cctv-point"><i class="fas fa-check-circle"></i> DVR/NVR Configuration</div>
                    <div class="cctv-point"><i class="fas fa-check-circle"></i> Remote Viewing Setup</div>
                    <div class="cctv-point"><i class="fas fa-check-circle"></i> Maintenance & Support</div>
                </div>
                <div class="hero-buttons" style="margin-top:2rem;">
                    <a href="/contact/" class="btn btn-primary btn-glow"><i class="fas fa-paper-plane"></i> Send Enquiry</a>
                    <a href="{{ site.phoneLink }}" class="btn btn-outline"><i class="fas fa-phone"></i> Call Now</a>
                </div>
            </div>
            <div class="cctv-visual" data-animate="fade-left">
                <img src="https://images.unsplash.com/photo-1557597774-9d273605dfa9?w=600&q=80" alt="CCTV Installation Services" loading="lazy" class="cctv-img">
                <div class="cctv-3d-icon">
                    <div class="cctv-cam-body"><i class="fas fa-video"></i></div>
                    <div class="cctv-scan-line"></div>
                </div>
            </div>
        </div>
    </div>
</section>

<!-- ========== 7. HARDWARE REPAIR SECTION ========== -->
<section id="hardware" class="hardware-section">
    <div class="container">
        <div class="section-header" data-animate="fade-up">
            <span class="section-tag">Hardware Services</span>
            <h2 class="section-title">IT Hardware <span class="gradient-text">Repair & Support</span></h2>
            <p class="section-desc">Expert repair services for laptops, desktops, and printers with quick turnaround and quality assurance.</p>
        </div>
        <div class="hardware-grid">
            <div class="hardware-card" data-animate="fade-up" data-delay="0">
                <img src="https://images.unsplash.com/photo-1588702547919-26089e690ecc?w=500&q=80" alt="Laptop Repair" loading="lazy">
                <div class="hardware-card-content"><h3><i class="fas fa-laptop"></i> Laptop Repair</h3><p>Screen replacement, motherboard repair, data recovery, and performance optimization.</p></div>
            </div>
            <div class="hardware-card" data-animate="fade-up" data-delay="100">
                <img src="https://images.unsplash.com/photo-1597872200969-2b65d56bd16b?w=500&q=80" alt="Desktop Repair" loading="lazy">
                <div class="hardware-card-content"><h3><i class="fas fa-desktop"></i> Desktop Repair</h3><p>Component upgrades, OS installation, virus removal, and complete system rebuilds.</p></div>
            </div>
            <div class="hardware-card" data-animate="fade-up" data-delay="200">
                <img src="https://images.unsplash.com/photo-1612815154858-60aa4c59eaa6?w=500&q=80" alt="Printer Service" loading="lazy">
                <div class="hardware-card-content"><h3><i class="fas fa-print"></i> Printer Service</h3><p>Inkjet & laser printer repair, toner replacement, network printer setup and maintenance.</p></div>
            </div>
        </div>
        <div class="hardware-cta" data-animate="fade-up">
            <a href="{{ site.phoneLink }}" class="btn btn-primary btn-glow"><i class="fas fa-phone"></i> Call Now</a>
            <a href="{{ site.whatsappLink }}" class="btn btn-whatsapp btn-glow" target="_blank"><i class="fab fa-whatsapp"></i> WhatsApp Now</a>
        </div>
    </div>
</section>

<!-- ========== 8. CLOUD + AI SECTION ========== -->
<section id="cloud-ai" class="cloud-section">
    <div class="cloud-bg"></div>
    <div class="container">
        <div class="cloud-grid">
            <div class="cloud-content" data-animate="fade-right">
                <span class="section-tag">Future Ready</span>
                <h2 class="section-title">Cloud Computing & <span class="gradient-text">AI Solutions</span></h2>
                <p>We help businesses with cloud backup, secure infrastructure, and modern IT support powered by cutting-edge AI technology.</p>
                <ul class="cloud-features">
                    <li><i class="fas fa-cloud-arrow-up"></i> Cloud Migration & Backup</li>
                    <li><i class="fas fa-brain"></i> AI-Powered Analytics</li>
                    <li><i class="fas fa-shield-halved"></i> Secure Infrastructure</li>
                    <li><i class="fas fa-server"></i> Scalable Solutions</li>
                    <li><i class="fas fa-chart-line"></i> Performance Monitoring</li>
                </ul>
                <a href="/contact/" class="btn btn-primary btn-glow"><i class="fas fa-paper-plane"></i> Send Enquiry</a>
            </div>
            <div class="cloud-visual" data-animate="fade-left">
                <div class="cloud-illustration">
                    <div class="cloud-shape"><i class="fas fa-cloud"></i></div>
                    <div class="ai-nodes">
                        <div class="ai-node node-1"><i class="fas fa-brain"></i></div>
                        <div class="ai-node node-2"><i class="fas fa-database"></i></div>
                        <div class="ai-node node-3"><i class="fas fa-chart-bar"></i></div>
                        <div class="ai-node node-4"><i class="fas fa-lock"></i></div>
                    </div>
                    <div class="data-particles">
                        <span class="particle p1"></span><span class="particle p2"></span><span class="particle p3"></span>
                        <span class="particle p4"></span><span class="particle p5"></span><span class="particle p6"></span>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<!-- ========== VIDEO SHOWCASE (MP4) ========== -->
<section id="video" style="padding:80px 0;background:linear-gradient(180deg,var(--navy),#0F1B2E);position:relative;overflow:hidden">
    <div style="position:absolute;inset:0;background:radial-gradient(ellipse at 50% 50%,rgba(37,99,235,.15),transparent 60%)"></div>
    <div class="container" style="position:relative;z-index:2">
        <div class="section-header" data-animate="fade-up" style="text-align:center">
            <span class="section-tag" style="background:rgba(255,255,255,.1);color:var(--cyan)">Watch & Learn</span>
            <h2 class="section-title" style="color:#fff">{{ site.videoTitle | default("See Us in <span class='gradient-text'>Action</span>") | safe }}</h2>
            <p class="section-desc" style="color:rgba(255,255,255,.6)">{{ site.videoSubtitle }}</p>
        </div>
        <div data-animate="fade-up" style="max-width:900px;margin:0 auto;border-radius:16px;overflow:hidden;box-shadow:0 20px 60px rgba(0,0,0,.4),0 0 40px rgba(37,99,235,.15);border:1px solid rgba(255,255,255,.1)">
            <video controls playsinline preload="metadata" poster="https://images.unsplash.com/photo-1451187580459-43490279c0fa?w=900&q=80" style="width:100%;display:block;background:#000;">
                <source src="{{ site.videoFile }}" type="video/mp4">
                <p style="color:#fff;text-align:center;padding:40px;">Your browser does not support the video tag. <a href="{{ site.videoFile }}" style="color:var(--cyan);">Download the video</a>.</p>
            </video>
        </div>
        <p style="text-align:center;color:rgba(255,255,255,.4);margin-top:16px;font-size:.85rem">
            <i class="fas fa-info-circle"></i> Upload your video via Admin Panel → Settings → Video File Path
        </p>
    </div>
</section>

<!-- ========== 9. PROJECTS / PORTFOLIO ========== -->
<section id="projects" class="projects-section">
    <div class="container">
        <div class="section-header" data-animate="fade-up">
            <span class="section-tag">Our Portfolio</span>
            <h2 class="section-title">Featured <span class="gradient-text">Projects</span></h2>
            <p class="section-desc">Explore our latest projects across web development, mobile apps, CCTV installations, and IT infrastructure.</p>
        </div>
        <div class="project-filters" data-animate="fade-up">
            <button class="filter-btn active" data-filter="all">All</button>
            <button class="filter-btn" data-filter="web">Web Apps</button>
            <button class="filter-btn" data-filter="mobile">Mobile Apps</button>
            <button class="filter-btn" data-filter="website">Websites</button>
            <button class="filter-btn" data-filter="cctv">CCTV</button>
            <button class="filter-btn" data-filter="server">Servers</button>
            <button class="filter-btn" data-filter="network">Networking</button>
        </div>
        <div class="projects-grid">
            {% for proj in projects.projects %}
            <div class="project-card" data-category="{{ proj.category }}" data-animate="fade-up">
                <div class="project-img">
                    <img src="{{ proj.image }}" alt="{{ proj.name }}" loading="lazy">
                    <div class="project-overlay">
                        <span class="project-category">{{ proj.categoryLabel }}</span>
                        <h4>{{ proj.name }}</h4>
                        <p style="color:rgba(255,255,255,.7);font-size:.85rem;margin-bottom:10px">{{ proj.desc }}</p>
                        <a href="/contact/" class="btn btn-sm">Send Enquiry</a>
                    </div>
                </div>
            </div>
            {% endfor %}
        </div>
    </div>
</section>

<!-- ========== 10. TESTIMONIALS ========== -->
<section id="testimonials" class="testimonials-section">
    <div class="container">
        <div class="section-header" data-animate="fade-up">
            <span class="section-tag">Client Reviews</span>
            <h2 class="section-title">What Our <span class="gradient-text">Clients Say</span></h2>
            <p class="section-desc">Trusted by businesses across India and worldwide for reliable IT solutions.</p>
        </div>
        {% if testimonials.testimonials | length > 0 %}
        <div class="testimonials-grid">
            {% for review in testimonials.testimonials %}
            <div class="testimonial-card" data-animate="fade-up" data-delay="{{ loop.index0 * 100 }}">
                <div class="testimonial-stars">
                    {% for i in range(0, review.stars) %}<i class="fas fa-star"></i>{% endfor %}
                    {% for i in range(0, 5 - review.stars) %}<i class="far fa-star"></i>{% endfor %}
                </div>
                <p class="testimonial-text">"{{ review.text }}"</p>
                <div class="testimonial-author">
                    <div class="author-avatar">{{ review.initials }}</div>
                    <div><h4>{{ review.name }}</h4><span>{{ review.position }}</span></div>
                </div>
            </div>
            {% endfor %}
        </div>
        {% else %}
        <div class="testimonials-grid" data-animate="fade-up" style="display:flex;justify-content:center;align-items:center;min-height:250px;">
            <div style="text-align:center;padding:3rem 2rem;">
                <div style="font-size:3.5rem;margin-bottom:1rem;opacity:0.15;"><i class="fas fa-quote-left"></i></div>
                <h3 style="font-size:1.4rem;color:var(--navy);margin-bottom:0.75rem;font-weight:600;">Reviews Coming Soon</h3>
                <p style="color:var(--text-light);max-width:450px;margin:0 auto 1.5rem;line-height:1.7;">
                    We're a new company building amazing IT solutions. Be our first client and share your experience with us!
                </p>
                <a href="/contact/" class="btn btn-primary btn-glow"><i class="fas fa-paper-plane"></i> Get Started With Us</a>
            </div>
        </div>
        {% endif %}
    </div>
</section>

<!-- ========== 11. CONSULTATION FORM ========== -->
<section id="consultation" class="consultation-section">
    <div class="container">
        <div class="consultation-grid">
            <div class="consultation-info" data-animate="fade-right">
                <span class="section-tag">Get In Touch</span>
                <h2 class="section-title">Request a <span class="gradient-text">Free Consultation</span></h2>
                <p>Tell us about your project requirements and our expert team will get back to you within 30 minutes during business hours.</p>
                <div class="consultation-contact">
                    <div class="consult-contact-item">
                        <i class="fas fa-phone"></i>
                        <div><h4>Call Us</h4><a href="{{ site.phoneLink }}">{{ site.phone }}</a></div>
                    </div>
                    <div class="consult-contact-item">
                        <i class="fab fa-whatsapp"></i>
                        <div><h4>WhatsApp</h4><a href="{{ site.whatsappLink }}" target="_blank">{{ site.whatsapp }}</a></div>
                    </div>
                    <div class="consult-contact-item">
                        <i class="fas fa-envelope"></i>
                        <div><h4>Email</h4><a href="mailto:{{ site.email }}">{{ site.email }}</a></div>
                    </div>
                    <div class="consult-contact-item">
                        <i class="fas fa-location-dot"></i>
                        <div><h4>Location</h4><span>{{ site.address }}</span></div>
                    </div>
                </div>
            </div>
            <div class="consultation-form-wrapper" data-animate="fade-left">
                <form class="consultation-form" name="consultation" method="POST" data-netlify="true" netlify-honeypot="bot-field">
                    <input type="hidden" name="form-name" value="consultation">
                    <input type="hidden" name="bot-field">
                    <div class="form-group"><label for="name">Full Name *</label><input type="text" id="name" name="full_name" placeholder="Your name" required></div>
                    <div class="form-group"><label for="company">Company Name</label><input type="text" id="company" name="company" placeholder="Company name (optional)"></div>
                    <div class="form-row">
                        <div class="form-group"><label for="phone">Phone Number *</label><input type="tel" id="phone" name="phone" placeholder="+91" required></div>
                        <div class="form-group"><label for="email">Email Address *</label><input type="email" id="email" name="email" placeholder="your@email.com" required></div>
                    </div>
                    <div class="form-group">
                        <label for="service">Service Required</label>
                        <select id="service" name="service">
                            <option value="" disabled selected>Select a service</option>
                            {% for svc in services.services %}
                            <option>{{ svc.name }}</option>
                            {% endfor %}
                        </select>
                    </div>
                    <div class="form-group"><label for="message">Message</label><textarea id="message" name="message" rows="5" placeholder="Tell us about your requirements..."></textarea></div>
